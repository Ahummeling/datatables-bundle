{
    "name": "omines/datatables-bundle",
    "type": "symfony-bundle",
    "description": "Symfony DataTables Bundle",
    "keywords": ["symfony","jquery","datatable", "datatables", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name":   "Robbert Beesems"
        }
    ],
    "require": {
        "php": "^7.0.8",
        "symfony/framework-bundle": "^3.0",
        "symfony/property-access": "^3.0",
        "symfony/translation": "^3.3",
        "symfony/yaml": "^3.0",
        "twig/twig": "~1.8|^2"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "friendsofphp/php-cs-fixer": "^2.7",
        "phpunit/phpunit": "^6.4"
    },
    "suggest": {
        "doctrine/orm": "For full automated integration with Doctrine entities"
    },
    "autoload": {
        "psr-4": { "Omines\\DatatablesBundle\\": "src/"}
    },
    "autoload-dev": {
      "psr-4": { "Tests\\": "tests/"}
    },
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
